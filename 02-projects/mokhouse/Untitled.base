views:
  - type: table
    name: Table
    filters:
      and:
        - relation.contains(link("mokhouse-projects"))
        - file.name != "mok-house-project"
    order:
      - file.name
      - relation
      - APRA
      - Date Paid
      - date received
      - demo fee
      - due date
      - internal review
      - "Invoice #"
      - paid
      - PO
      - awarded
      - award fee
      - status
    columnSize:
      file.name: 212
