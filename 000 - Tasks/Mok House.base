views:
  - type: table
    name: Table
    filters:
      and:
        - relation.contains(link("mokhouse"))
        - type == "Task"
        - Done != true
    order:
      - Done
      - file.name
      - relation
      - effort
    sort:
      - property: relation
        direction: ASC
    columnSize:
      file.name: 387
  - type: cards
    name: View
    order:
      - file.name
      - file.backlinks
      - file.basename
      - file.ctime
      - file.embeds
      - file.ext
