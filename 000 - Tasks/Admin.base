views:
  - type: table
    name: Table
    filters:
      and:
        - relation.contains(link("admin"))
        - Done != true
    order:
      - Done
      - file.name
      - description
      - date created
      - relation
      - today
      - follow up
      - effort
    sort:
      - property: date
        direction: ASC
    columnSize:
      file.name: 256
      note.description: 282
