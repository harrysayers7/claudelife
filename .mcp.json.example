{
  "mcpServers": {
    "notion": {
      "command": "npx",
      "args": ["-y", "@notionhq/client"],
      "env": {
        "NOTION_API_TOKEN": "$NOTION_API_TOKEN"
      }
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-supabase"],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "$SUPABASE_ACCESS_TOKEN"
      }
    },
    "task-master-ai": {
      "command": "npx",
      "args": ["-y", "--package=task-master-ai", "task-master-ai"],
      "env": {
        "ANTHROPIC_API_KEY": "$ANTHROPIC_API_KEY",
        "PERPLEXITY_API_KEY": "$PERPLEXITY_API_KEY"
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$GITHUB_PERSONAL_ACCESS_TOKEN"
      }
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "shadcn-ui": {
      "command": "npx",
      "args": ["shadcn-mcp"]
    },
    "exa": {
      "command": "npx",
      "args": ["-y", "@upsolve/exa-mcp"],
      "env": {
        "EXA_API_KEY": "$EXA_API_KEY"
      }
    },
    "n8n": {
      "command": "npx",
      "args": ["-y", "@upsolve/n8n-mcp"]
    },
    "awesome-n8n-templates": {
      "command": "npx",
      "args": ["-y", "@upsolve/awesome-n8n-templates-mcp"]
    },
    "graphiti-personal": {
      "command": "uvx",
      "args": ["--from", "graphiti-mcp", "graphiti"],
      "env": {
        "NEO4J_URI": "neo4j+s://your-neo4j-uri.databases.neo4j.io",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "$NEO4J_PASSWORD",
        "OPENAI_API_KEY": "$OPENAI_API_KEY",
        "GRAPHITI_DEFAULT_GROUP_ID": "personal"
      }
    },
    "graphiti-finance": {
      "command": "uvx",
      "args": ["--from", "graphiti-mcp", "graphiti"],
      "env": {
        "NEO4J_URI": "neo4j+s://your-neo4j-uri.databases.neo4j.io",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "$NEO4J_PASSWORD",
        "OPENAI_API_KEY": "$OPENAI_API_KEY",
        "GRAPHITI_DEFAULT_GROUP_ID": "finance"
      }
    },
    "graphiti-mokai": {
      "command": "uvx",
      "args": ["--from", "graphiti-mcp", "graphiti"],
      "env": {
        "NEO4J_URI": "neo4j+s://your-neo4j-uri.databases.neo4j.io",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "$NEO4J_PASSWORD",
        "OPENAI_API_KEY": "$OPENAI_API_KEY",
        "GRAPHITI_DEFAULT_GROUP_ID": "mokai"
      }
    },
    "graphiti-mok-house": {
      "command": "uvx",
      "args": ["--from", "graphiti-mcp", "graphiti"],
      "env": {
        "NEO4J_URI": "neo4j+s://your-neo4j-uri.databases.neo4j.io",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "$NEO4J_PASSWORD",
        "OPENAI_API_KEY": "$OPENAI_API_KEY",
        "GRAPHITI_DEFAULT_GROUP_ID": "mok-house"
      }
    },
    "graphiti-ai-brain": {
      "command": "uvx",
      "args": ["--from", "graphiti-mcp", "graphiti"],
      "env": {
        "NEO4J_URI": "neo4j+s://your-neo4j-uri.databases.neo4j.io",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "$NEO4J_PASSWORD",
        "OPENAI_API_KEY": "$OPENAI_API_KEY",
        "GRAPHITI_DEFAULT_GROUP_ID": "ai-brain"
      }
    },
    "graphiti-claudelife": {
      "command": "uvx",
      "args": ["--from", "graphiti-mcp", "graphiti"],
      "env": {
        "NEO4J_URI": "neo4j+s://your-neo4j-uri.databases.neo4j.io",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "$NEO4J_PASSWORD",
        "OPENAI_API_KEY": "$OPENAI_API_KEY",
        "GRAPHITI_DEFAULT_GROUP_ID": "claudelife"
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upsolve/context7"],
      "env": {
        "CONTEXT7_API_KEY": "$CONTEXT7_API_KEY"
      }
    },
    "upbank": {
      "command": "npx",
      "args": ["-y", "@upsolve/upbank-mcp"],
      "env": {
        "UPBANK_API_TOKEN": "$UPBANK_API_TOKEN"
      }
    },
    "fastmcp-brain": {
      "command": "uvx",
      "args": ["--from", "fastmcp[mcp]", "fastmcp", "run", "/Users/harrysayers/Developer/brain/fastmcp/brain.py"],
      "env": {
        "ANTHROPIC_API_KEY": "$ANTHROPIC_API_KEY"
      }
    },
    "claudelife-business-api": {
      "command": "uvx",
      "args": ["--from", "fastmcp[mcp]", "fastmcp", "dev", "/Users/harrysayers/Developer/claudelife/api/business/main.py"],
      "env": {
        "OPENAI_API_KEY": "$OPENAI_API_KEY"
      }
    },
    "claudelife-financial-api": {
      "command": "uvx",
      "args": ["--from", "fastmcp[mcp]", "fastmcp", "dev", "/Users/harrysayers/Developer/claudelife/api/finance/main.py"],
      "env": {
        "SUPABASE_URL": "https://your-project.supabase.co",
        "SUPABASE_KEY": "$SUPABASE_ANON_KEY",
        "MINDSDB_URL": "http://127.0.0.1:47334"
      }
    },
    "mindsdb": {
      "command": "npx",
      "args": ["-y", "@upsolve/mindsdb-mcp"],
      "env": {
        "MINDSDB_URL": "http://127.0.0.1:47334"
      }
    },
    "docker": {
      "command": "npx",
      "args": ["-y", "@upsolve/docker-mcp"]
    },
    "executeautomation-playwright-server": {
      "command": "npx",
      "args": ["-y", "@executeautomation/playwright-mcp-server"]
    }
  }
