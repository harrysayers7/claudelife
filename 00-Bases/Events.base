views:
  - type: table
    name: Table
    filters:
      or:
        - type.contains("event")
        - '!note["internal review"].isEmpty()'
    order:
      - file.name
      - when
      - event
      - internal review
      - date received
      - Date Paid
    sort:
      - property: when
        direction: ASC
    columnSize:
      file.name: 312
      note.when: 128
      note.event: 212
