views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder.contains("07-context/systems")
        - file.name != "README"
    order:
      - file.name
      - status
      - type
      - location
      - relation
      - system_type
      - created
      - description
    sort:
      - property: created
        direction: ASC
      - property: file.ctime
        direction: DESC
    columnSize:
      note.status: 114
      note.type: 126
      note.location: 122
