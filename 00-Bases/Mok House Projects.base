views:
  - type: table
    name: Table
    filters:
      and:
        - relation.contains(link("mokhouse"))
        - type.contains("project")
    order:
      - project name
      - status
      - demo fee
      - Date Paid
      - award fee
      - paid
      - "Invoice #"
      - invoice sent
      - date received
      - due date
      - file.name
      - awarded
      - internal review
      - PO
      - submitted date
      - APRA
      - id
    sort:
      - property: date received
        direction: DESC
      - property: demo fee
        direction: DESC
    columnSize:
      note.project name: 168
      note.due date: 154
      file.name: 217
