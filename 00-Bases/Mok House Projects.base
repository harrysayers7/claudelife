views:
  - type: table
    name: Table
    filters:
      and:
        - relation.contains(link("mokhouse-projects"))
        - file.name != "mok-house-project"
    order:
      - project name
      - demo fee
      - award fee
      - paid
      - "Invoice #"
      - invoice sent
      - date received
      - due date
      - file.name
      - awarded
      - Date Paid
      - internal review
      - PO
      - submitted date
      - APRA
    sort:
      - property: date received
        direction: DESC
      - property: demo fee
        direction: DESC
    columnSize:
      note.project name: 168
      file.name: 217
